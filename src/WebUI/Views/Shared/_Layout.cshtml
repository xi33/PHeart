@using StackExchange.Profiling
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <title>@ViewBag.Title | 心灵相约</title>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/css/html5-base.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/css/header.css")" />
    @RenderSection("Content", false)
</head>
<body>
    <div id="page-wrap">
        <div id="header">
            <div id="header_nav">
                <ul>
                    <li><a href="@Url.Action("Index","Home")"><strong>首页</strong></a></li>
                    @*<li><a href="#"><strong>心闻快报</strong></a></li>
                    <li><a href="#"><strong>中心简介</strong></a></li>
                    <li><a href="#"><strong>心理辅导</strong></a></li>
                    <li><a href="#"><strong>心理百科</strong></a></li>
                    <li><a href="#"><strong>影音图片</strong></a></li>
                    <li><a href="#"><strong>资源下载</strong></a></li>
                    <li><a href="#"><strong>心理自助中心</strong></a></li>
                    <li><a href="#"><strong>心理课堂</strong></a></li>*@
                    @foreach (var fst in ViewBag.FstClasses)
                    {
                        <li><a href="@Url.Action("List", "News", new { fstClassId = fst.Id })"><strong>@fst.Name</strong></a></li>
                    }
                </ul>
            </div>
        </div>
        @RenderBody()
    </div>
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    @RenderSection("Scripts", false)
    @MiniProfiler.RenderIncludes()
</body>
</html>
